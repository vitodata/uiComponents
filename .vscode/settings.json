{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/app/**/*.js": true,
        "**/e2e/**/*.js": true
    },
    "files.defaultLanguage": "typescript",
    "files.hotExit": "off",
    "workbench.iconTheme": "vscode-icon-theme",
    "files.trimTrailingWhitespace": true,
    "prettier.singleQuote": true,
    "editor.formatOnSave": true
}
